{
  "name": "sehat-mitra-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Sehat Mitra Healthcare Management System - Full Stack Application",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "start": "concurrently \"npm run backend:start\" \"npm run frontend:dev\"",
    "build": "npm run frontend:build",
    "preview": "npm run frontend:preview",
    
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:preview": "cd frontend && npm run preview",
    "frontend:install": "cd frontend && npm install",
    "frontend:test": "cd frontend && npm test",
    
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "backend:install": "cd backend && npm install",
    "backend:test": "cd backend && npm test",
    
    "install:all": "npm run frontend:install && npm run backend:install",
    "test:all": "npm run frontend:test && npm run backend:test",
    "clean": "rm -rf frontend/node_modules backend/node_modules frontend/dist",
    "setup": "npm run install:all"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "author": "Ayush Jha",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/sehat-mitra.git"
  },
  "keywords": [
    "healthcare",
    "telemedicine",
    "appointment-booking",
    "react",
    "nodejs",
    "mongodb"
  ]
}
